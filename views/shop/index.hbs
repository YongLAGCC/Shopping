{{# each products}}

    <div class="row">
    {{# each this}}

    <div class="col-sm-6 col-md-4">
    <div class="thumbnail">
        <img src={{this.imagePath }}
        alt="" class = "img-responsive">
        <div class="caption">
        <h3>{{this.title}}</h3>
        <p class="description"> 
            {{this.description}}   
        <p>
            <div class="clearfix">
                <a href="#" class="price  pull-left">${{this.price}}</a> 
                <a href="#" class="btn btn-success pull-right" role="button">Add In Shopping Car</a>
            </div>  
        </p>
        </div>
    </div>
    </div>

    {{/each}}
    </div>
{{/each}}



